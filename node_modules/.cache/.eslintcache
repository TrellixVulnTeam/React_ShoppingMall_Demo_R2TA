div>\n        ) : null\n      }\n\n      <div className=\"row\">\n        <div className=\"col-md-6\">\n          <img\n            src={\n              \"https://codingapple1.github.io/shop/shoes\" +\n              (selectItem.id + 1) +\n              \".jpg\"\n            }\n            width=\"100%\"\n            alt=\"이미지\"\n          />\n        </div>\n        <div className=\"col-md-6 mt-4\">\n          <h4 className=\"pt-5\">{selectItem.title}</h4>\n          <p>{selectItem.content}</p>\n          <p>{selectItem.price}원</p>\n          <Info stock={props.stock} selectItem={selectItem}></Info>\n          <button\n            className=\"btn btn-danger\"\n            onClick={() => {\n              props.stockChange(stockMinus);\n            }}\n          >\n            주문하기\n          </button>\n          &nbsp;\n          <button\n            className=\"btn btn-danger\"\n            onClick={() => {\n              history.push(\"/\");\n            }}\n          >\n            뒤로가기\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nfunction Info(props) {\n  return <p>재고 : {props.stock[props.selectItem.id]}</p>;\n}\n\nexport default Detail;\n","/Users/kimjunghwan/coding apple/REACT CLASS/shop/src/data.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":22,"column":5,"nodeType":"37","messageId":"38","endLine":22,"endColumn":13},{"ruleId":"35","severity":1,"message":"39","line":27,"column":7,"nodeType":"37","messageId":"38","endLine":27,"endColumn":14},{"ruleId":"35","severity":1,"message":"40","line":1,"column":8,"nodeType":"37","messageId":"38","endLine":1,"endColumn":13},{"ruleId":"41","severity":1,"message":"42","line":70,"column":17,"nodeType":"43","messageId":"44","endLine":70,"endColumn":19},"no-native-reassign",["45"],"no-negated-in-lhs",["46"],"no-unused-vars","'Pointer2' is assigned a value but never used.","Identifier","unusedVar","'history' is assigned a 